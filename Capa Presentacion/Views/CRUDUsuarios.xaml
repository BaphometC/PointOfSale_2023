            <TextBlock x:Name="Titulo"
                       Text="Creacion de Usuarios"
                       FontWeight="SemiBold"
                       Grid.Column="2"
                       Grid.ColumnSpan="3"
                       Grid.Row="1"
                       FontSize="30"
                       VerticalAlignment="Center"
                       HorizontalAlignment="Center"/>
            <!--#endregion-->
            <!--#region Botones-->
            <!--#region Regresar-->
            <Button Style="{DynamicResource BtnStyle}" x:Name="BtnRegresar"
                    Content="Atras"
                    FontSize="17"
                    Foreground="#42464b"
                    Grid.Column="1"
                    Grid.Row="1"
                    Cursor="Hand"
                    Background="Transparent"
                    Click="Regresar"
                    Margin="10,48,150,28"
                    Grid.RowSpan="3"/>
            <!--#endregion-->
            <!--#region Crear-->
            <Button Style="{DynamicResource BtnCrear}" x:Name="BtnCrear"
                    Content="Crear Usuario"
                    Grid.Row="13"
                    Grid.Column="2"
                    Foreground="White"
                    FontWeight="SemiBold"
                    VerticalContentAlignment="Center"
                    Background="#008200" Margin="299,32,15,482"
                    Visibility="Hidden"
                    Cursor="Hand"
                    Click="Crear"/>
            <!--#endregion-->
            <!--#region Eliminar-->
            <Button Style="{DynamicResource BtnELiminar}" x:Name="BtnEliminar"
                    Content="Eliminar Usuario"
                    Grid.Row="13"
                    Grid.Column="2"
                    Foreground="White"
                    FontWeight="SemiBold"
                    VerticalContentAlignment="Center"
                    Background="#BB0000" Margin="299,32,15,482"
                    Visibility="Hidden"
                    Cursor="Hand"
                    Click="Eliminar"/>
            <!--#endregion-->
            <!--#region Actualizar-->
            <Button Style="{DynamicResource BtnActualizar}" x:Name="BtnActualizar"
                    Content="Actualizar Usuario"
                    Grid.Row="13"
                    Grid.Column="2"
                    Foreground="Black"
                    FontWeight="SemiBold"
                    VerticalContentAlignment="Center"
                    Background="#FBD303" Margin="299,32,15,482"
                    Visibility="Hidden"
                    Cursor="Hand"
                    Click="Actualizar"/>
            <!--#endregion-->
            <!--#endregion-->
            <!--#region Contenido-->
            <TextBlock Grid.Row="4"
                       Grid.Column="1"
                       Margin="150,0,0,0"
                       Text="Nombre:"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"/>
            <TextBox Style="{DynamicResource TextBox}"
                     HorizontalContentAlignment="Center"
                     Grid.Row="6"
                     Grid.Column="1"
                     x:Name="tbNombre" Grid.ColumnSpan="2" 
                     Margin="152,3,414,2" Grid.RowSpan="2"/>
            <TextBlock Grid.Row="4"
                       Text="Apellido:"
                       Margin="145,4,0,0"
                       Grid.Column="2"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top"/>
            <TextBox Style="{DynamicResource TextBox}"
                     HorizontalContentAlignment="Center"
                     Grid.Row="6"
                     Grid.Column="2"
                     x:Name="tbApellido" 
                     Margin="146,3,160,2" Grid.RowSpan="2"/>
            <TextBlock Grid.Row="4"
                       Text="Dni:"
                       Margin="387,5,0,0"
                       Grid.Column="2"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top" Grid.RowSpan="2"/>
            <TextBox Style="{DynamicResource TextBox}"
                     HorizontalContentAlignment="Center"
                     Grid.Row="6"
                     Grid.Column="2"
                     x:Name="tbDni" 
                     Margin="385,2,204,3" Grid.ColumnSpan="3" Grid.RowSpan="2"/>
            <TextBlock Grid.Row="4"
                       Text="Correo:"
                       Margin="73,0,0,0"
                       Grid.Column="4"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center" Grid.RowSpan="2"/>
            <TextBox Style="{DynamicResource TextBox}"
                     HorizontalContentAlignment="Center"
                     Grid.Row="6"
                     Grid.Column="4"
                     x:Name="tbCorreo" Grid.ColumnSpan="2" Margin="76,1,129,4" Grid.RowSpan="2"/>
            <TextBlock Grid.Row="8"
                       Text="Id Rol:"
                       Margin="158,15,0,0"
                       Grid.Column="1"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Top" Grid.RowSpan="3"/>
            <ComboBox Style="{DynamicResource ComboBox}"
                      Grid.Row="10"
                      HorizontalContentAlignment="Center"
                      Grid.Column="1"
                      x:Name="tbIDRol"
                      Cursor="Hand"
                      BorderBrush="White"
                      Foreground="Black" Grid.ColumnSpan="2" Margin="151,12,419,23" Grid.RowSpan="3">
                <ComboBox.ItemContainerStyle>
                    <Style TargetType="{x:Type ComboBoxItem}">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="{x:Type ComboBoxItem}">
                                    <Border CornerRadius="10" SnapsToDevicePixels="True"
                                            Background="{TemplateBinding Background}"
                                            BorderBrush="{TemplateBinding BorderBrush}"
                                            BorderThickness="{TemplateBinding BorderThickness}"
                                            Padding="{TemplateBinding Padding}">
                                        <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                                    </Border>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsHighlighted" Value="True">
                                            <Setter Property="Background" Value="#718787"/>
                                            <Setter Property="BorderBrush" Value="#718787"/>
                                            <Setter Property="Foreground" Value="White"/>
                                        </Trigger>
                                        <Trigger Property="IsHighlighted" Value="False">
                                            <Setter Property="Foreground" Value="Black"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </ComboBox.ItemContainerStyle>
            </ComboBox>
            <!--#endregion Contenido-->
        </Grid>
    </Border>
</Page>
